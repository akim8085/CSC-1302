package todo;

public class Events {
	int year;
	int month;
	int day;
	String eventName;
	String desc;
	
	public int getYear() {
		return year;
	}
		public int getMonth() {
		return month;
	}
	public int getDay() {
		return day;
	}

	
	public void setYear(int year) {
		this.year = year;
	}
	public void setMonth(int month) {
		this.month = month;
	}
	public void setDay(int day) {
		this.day = day;
	}
	
	
	
	
}
